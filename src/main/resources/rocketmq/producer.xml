<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="defaultProducer" class="com.kute.demo.mq.rocketmq.producer.DefaultProducer">
        <constructor-arg name="group" value="default_group" />
        <constructor-arg name="messageListener" ref="defaultMessageListener" />
    </bean>

    <bean id="defaultMessageListener" class="com.kute.demo.mq.rocketmq.listener.DefaultMessageListener">

    </bean>

</beans>